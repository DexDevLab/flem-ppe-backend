{
  "name": "flem-ppe-backend",
  "description": "Aplicação Backend para o Projeto Primeiro Emprego, da FLEM",
  "version": "v1.0.0-230623",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "docs": "jsdoc --readme README.md -c jsdoc-config.json",
    "db-push:dev": "dotenv -e .env.development -- yarn prisma db push",
    "db-seed:dev": "dotenv -e .env.development -- yarn prisma db seed",
    "studio:dev": "dotenv -e .env.development -- yarn prisma studio",
    "db:push": "dotenv -e .env.development -- yarn prisma db push",
    "migrate": "prisma migrate dev",
    "caniuse": "npm update --save caniuse-lite browserslist",
    "yarn:clean": "yarn autoclean --force",
    "git:clear": "bash cleargitcache.sh",
    "docker:build": "docker build . -t ghcr.io/frtechdev/flem-ppe-backend:latest",
    "docker:push": "docker push ghcr.io/frtechdev/flem-ppe-backend:latest"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.3.16",
    "@prisma/client": "^4.15.0",
    "axios": "^0.27.2",
    "browserslist": "^4.21.7",
    "clean-jsdoc-theme": "^3.3.4",
    "google-libphonenumber": "^3.2.27",
    "googleapis": "^105.0.0",
    "jsdoc": "^3.6.10",
    "lodash": "^4.17.21",
    "luxon": "^3.0.1",
    "masks-br": "^1.0.1",
    "multer": "^1.4.5-lts.1",
    "next": "12.1.6",
    "next-auth": "^4.3.4",
    "next-connect": "^0.12.2",
    "node-quill-converter": "^0.3.3",
    "nodemailer": "^6.7.6",
    "ntp-client": "^0.5.3",
    "pdf-merger-js": "^4.3.0",
    "prisma-dbml-generator": "^0.10.0",
    "puppeteer": "^19.3.0",
    "qs": "^6.11.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "dotenv-cli": "^7.2.1",
    "eslint": "8.16.0",
    "eslint-config-next": "12.1.6",
    "prisma": "^4.15.0"
  }
}
